{
  "name": "ali_news_be",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@utils": "dist/utils",
    "@controller": "dist/controller",
    "@routes": "dist/routes",
    "@crawler": "dist/crawler",
    "@mongodb": "dist/MongoDb",
    "@loadenv": "dist/LoadEnv"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "mongoose": "^5.9.26",
    "p-limit": "^3.0.2",
    "unidecode": "^0.1.8",
    "bcrypt": "^5.0.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "p-queue": "^6.6.0",
    "randomstring": "^1.1.5",
    "winston": "^3.3.3",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.21",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.12",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/nodemon": "^1.19.0",
    "@types/p-limit": "^2.2.0",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.10",
    "@types/unidecode": "^0.1.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^3.9.7"
  }
}
